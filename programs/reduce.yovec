// This is an expression reduction test program for Yovec

let number ADD_ZERO = (0 + 3) + (3 + 0)
export ADD_ZERO

let number SUB_ZERO = 3 - 0
export SUB_ZERO

let number MUL_ZERO = (0 * 3) + (3 * 0)
export MUL_ZERO

let number MUL_ONE = (1 * 3) + (3 * 1)
export MUL_ONE

let number DIV_ONE = 3 / 1
export DIV_ONE

let number EXP_ZERO = (0 ^ 3) + (3 ^ 0)
export EXP_ZERO

let number EXP_ONE = (1 ^ 3) + (3 ^ 1)
export EXP_ONE

let number BINARY_OP = (1 + 2) * 3
export BINARY_OP
